<!doctype html>
<html lang="ru">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>Selectel Photo Gallery</title>
  <meta name="description" content="">
  <meta content="NOINDEX, NOFOLLOW" name="robots">
  <meta name="author" content="Andrew Vlasov">
  <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1">
  <!-- <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0"> -->
  <meta name="apple-mobile-web-app-capable" content="yes" />
  <style type="text/css"><%- fs.readFileSync('.temp/css/style.css') %></style>
</head>
<body>
  <div class="header">
    <a href="http://storage.selectel.ru/vozmozhnosti/fotogalereya/" style="margin-left: 2em; margin-top: .2em; position: absolute;">
      <%- fs.readFileSync('./src/img/svg/selectel.svg') %>
      <div class="before-load"></div>
    </a>
    <div class="count"></div>
    <a class="save-all" title="Скачать всё!" href="?download-all-as-zip=gallery.zip">
      <svg viewBox="0 0 24 24"><path d="M12,19L7,14H10V10H14V14H17M10,4H14V6H10M20,6H16V4L14,2H10L8,4V6H4C2.89,6 2,6.89 2,8V19A2,2 0 0,0 4,21H20A2,2 0 0,0 22,19V8C22,6.89 21.1,6 20,6Z"></path></svg>
    </a>
  </div>
  <div class="wrapper">
    <div class="content">
      <div class="photo-list">
      </div>
      <div id="lazy">
        <div class="btn">more..!</div>
      </div>
    </div>
  </div>
  <div class="light-box">
    <div class="light-box-image">
    </div>

    <a href="#" class="btn-fullscreen icon-fullscreen">
      <%- fs.readFileSync('./src/img/svg/fullscreen.svg') %>
    </a>

    <div class="light-box-footer">
      <a href="#" class="btn-prev">
        <span class="icon-left"><%- fs.readFileSync('./src/img/svg/left.svg') %></span>
      </a>
      <a href="#" class="btn-play">
        <!-- <span style="width: 28px; height: 30px; position: relative;"> -->
          <span class="icon-play"><%- fs.readFileSync('./src/img/svg/play.svg') %></span>
          <span class="icon-pause"><%- fs.readFileSync('./src/img/svg/pause.svg') %></span>
        <!-- </span> -->
      </a>
      <a href="#" class="btn-next">
        <span class="icon-right"><%- fs.readFileSync('./src/img/svg/right.svg') %></span>
      </a>
      <a href="#" class="btn-share">
        <span class="icon-share"><%- fs.readFileSync('./src/img/svg/share.svg') %></span>
      </a>
      <a href="#" class="btn-close">
        <span class="icon-close"><%- fs.readFileSync('./src/img/svg/close.svg') %></span>
      </a>
    </div>
  </div>
  <div class="share">
    <a target="_blank" class="tw" href="#" rel="nofollow">
      <span class="icon-twitter" title="Поделиться в twitter">
        <img src='<%- encodeImg("./src/img/svg/twitter.svg") %>'/>
      </span></a>
    <a target="_blank" class="vk" href="#" rel="nofollow">
      <span class="icon-vk" title="Поделиться в Вконтакте">
        <img src='<%- encodeImg("./src/img/svg/vk.svg") %>'/>
      </span></a>
    <a target="_blank" class="fb" href="#" rel="nofollow">
      <span class="icon-facebook" title="Поделиться в Вконтакте">
        <img src='<%- encodeImg("./src/img/svg/facebook.svg") %>'/>
      </span></a>
    <div class="br-fix"></div>
    <a target="_blank" class="gp" href="#" rel="nofollow">
      <span class="icon-gplus" title="Поделиться в Google+">
        <img src='<%- encodeImg("./src/img/svg/gplus.svg") %>'/>
      </span></a>
    <!-- <a target="_blank" class="mail" href="#" rel="nofollow">
      <span class="icon-email" title="Отправить по email">
        <img src=''/>
      </span></a> -->
    <a target="_blank" class="dl" href="#" rel="nofollow">
      <span class="icon-download" title="Посмотреть оригинал">
        <img src='<%- encodeImg("./src/img/svg/download.svg") %>'/>
      </span></a>
  </div>
  <!-- end .wrapper-->
</body>
<% if (DEV) { %>
  <script>var FLD = 'test-gallery/', HOST = 'http://64815.selcdn.ru/', THUMBS_OFF = false;</script>
  <script src="vendor.js"  ></script>
  <script src="app.js" defer async></script>
<% } else { %>
  <script>var FLD = './', HOST = '', THUMBS_OFF = false;
  </script>
  <script><%- fs.readFileSync('.temp/js/gallery.min.js') %></script>
<% } %>
</html>
